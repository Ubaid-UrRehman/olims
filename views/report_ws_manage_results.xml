<openerp>
<data>
<template id="report_ws_manage_results_document">
    <t t-call="report.external_layout">
        <div class="page">
            <div class="row mt32">
                <div align="right">
                     <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s&amp;humanreadable=%s' % 
                ('Standard39', doc.Worksheet, 250, 50, 1)"/>
                </div>
            </div>
            <div class="row mt32">
                <div class="col-xs-12">
                     <h1><strong><p t-field="doc.Worksheet"/></strong></h1>
                </div>
            </div>
            <div class="row mt32">
                <div class="col-xs-12">
                    Created on
                     <span t-esc="doc.create_date"/> by <span t-esc="doc.create_uid.name"/>
                     <br/>
                     Printed on <span t-esc="time.strftime('%Y-%m-%d %H:%M:%S')"/> by <span t-esc="user.partner_id.name"/>
                     <br/>
                     Analysed by: <span t-esc="doc.Analyst.name"/>
                </div>
            </div>
            <section>
            </section>
            <!-- <h2>Summary</h2> -->
 
            <table class="table table-condensed">
                <tbody>
                    <tr>
                        <td t-foreach="doc.ManageResult" t-as="result">
                                <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s&amp;humanreadable=%s' %('Standard39', result.request_analysis_id.RequestID, 250, 50, 1)"/>
                        </td>
                    </tr>
                    <!-- <span t-esc="doc.ManageResult[0].request_analysis_id.RequestID"/> -->
                    <tr>
                        <td>Request ID</td>
                        <!-- <t > -->
                        <td t-foreach="doc.ManageResult" t-as="result">
                            <span t-esc="result.request_analysis_id.RequestID"/>
                        </td>
                        <!-- </t> -->
                    </tr>
                    <tr>
                        <td>Client</td>
                        <td t-foreach="doc.ManageResult" t-as="result">
                            <span t-esc="result.client.Name"/>
                        </td>
                    </tr>
                    <tr>
                        <td>Sample Type</td>
                        <td t-foreach="doc.ManageResult" t-as="result">
                            <span t-esc="result.sample_type.SampleType"/>
                        </td>
                    </tr>
                    <tr>
                        <td >Sample</td>
                        <td t-foreach="doc.ManageResult" t-as="result">
                            <span t-esc="result.sample.name"/>
                        </td>
                    </tr>
                    <tr>
                        <td >Sampling Date</td>
                        <td t-foreach="doc.ManageResult" t-as="result">
                            <span t-esc="result.sampling_date"/>
                        </td>
                    </tr>
                    <tr>
                        <td >Date Received</td>
                        <td t-foreach="doc.ManageResult" t-as="result">
                            <span t-esc="result.received_date"/>
                        </td>
                    </tr>
                </tbody>
            </table>
            <h2>Analyses</h2>
            <!-- <span t-if="doc.Lab_Manage_Result"> -->
                <!-- <h3>Lab Analyses</h3> -->
                <table class="table table-condensed">
                    <!-- <th>Analysis</th>
                    <th>Results</th>
                    <th>Value Range</th> -->
                    <tbody>
                        <t t-foreach="doc.ManageResult" t-as="m_result">
                            <tr>
                                <td>
                                    <t t-esc="m_result.analysis.Service"/>
                                </td>
                                <td>
                                    
                                </td>
                                <td>
                                    
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            <!-- </span> -->
            <br/>
            <br/>
            <br/>
            <!-- <span t-if="doc.Field_Manage_Result">
                <h3>Field Analyses</h3>
                <table class="table table-condensed">
                    <th>Analysis</th>
                    <th>Results</th>
                    <th>Value Range</th>
                    <tbody>
                        <t t-foreach="doc.Field_Manage_Result" t-as="f_result">
                            <tr>
                                <td>
                                    <t t-esc="f_result.Category.Category"/>
                                    <t t-esc="f_result.Service.Service"/>
                                </td>
                                <td>
                                    <t t-esc="f_result.Result"/>
                                </td>
                                <td>
                                    <t t-esc="f_result.Specifications"/>
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            </span> -->
            <!-- <div align="right">
                     <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s&amp;humanreadable=%s' % 
                ('Standard39', doc.Worksheet, 250, 50, 1)"/>
                </div> -->
        </div>
    </t>
</template>
 
<template id="report_ws_manage_results">
    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="doc">
            <t t-call="olims.report_ws_manage_results_document"/>
        </t>
    </t>
</template>
</data>
</openerp>